<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?scenebuilder-stylesheet BlackTheme.css?>

<AnchorPane id="AnchorPane" fx:id="anchorMain" maxHeight="150.0" maxWidth="250.0" minHeight="150.0" minWidth="240.0"
            prefHeight="150.0" prefWidth="240.0" styleClass="background" stylesheets="@css/light/LightTheme(LogIn).css"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.devprosony.core.controllers.ScreenRegistration">
   <children>
      <TextField fx:id="textFieldLogin" layoutX="14.0" layoutY="45.0" promptText="Login" />
      <PasswordField fx:id="textFieldPassword" layoutX="13.0" layoutY="78.0" promptText="Password" />
      <Button fx:id="buttonSign" layoutX="129.0" layoutY="112.0" mnemonicParsing="false" onAction="#clickButtonSingIn" prefHeight="25.0" prefWidth="100.0" text="ok" textFill="#000000a6" AnchorPane.rightAnchor="10.0" />
      <Button fx:id="buttonExit" layoutX="10.0" layoutY="112.0" mnemonicParsing="false" onAction="#clickButtonCansel"
              prefHeight="25.0" prefWidth="100.0" text="cansel" textFill="#000000a6" />
      <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="149.0" styleClass="label-bright" text="Registration"
             textFill="#000000a5" />
      <Label fx:id="progressLabel" layoutX="173.0" prefHeight="105.0" prefWidth="48.0" textAlignment="CENTER" textFill="#000000a6" />
   </children>
</AnchorPane>
