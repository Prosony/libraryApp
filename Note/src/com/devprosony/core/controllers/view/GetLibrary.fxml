<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="600.0" maxWidth="290.0" minHeight="400.0" minWidth="290.0" prefHeight="400.0" prefWidth="290.0" styleClass="background" stylesheets="@css/generalCSS/BlackTheme(General).css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devprosony.core.controllers.modal.controller.ScreenLibraryManager">
   <children>
      <Button layoutX="178.0" layoutY="346.0" mnemonicParsing="false" onAction="#openLibrary" prefHeight="40.0" prefWidth="98.0" text="Open" AnchorPane.bottomAnchor="14.0" />
      <Button layoutX="264.0" layoutY="1.0" mnemonicParsing="false" onAction="#pressCancel" prefHeight="25.0" prefWidth="25.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0" />

       <TableView fx:id="tableListLibrary" layoutX="13.0" layoutY="17.0" prefHeight="382.0" prefWidth="157.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
        <columns>
               <TableColumn fx:id="nameLibrary" prefWidth="150.0" text="Library" />
        </columns>
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem mnemonicParsing="false" onAction="#contextMenuCreatLibrary" text="Create Library" />
                <MenuItem mnemonicParsing="false" onAction="#contextMenuRenameLibrary" text="Rename Library" />
                  <MenuItem mnemonicParsing="false" onAction="#contextMenuDeleteLibrary" text="Delete Library" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TableView>

   </children>
</AnchorPane>
